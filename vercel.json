{
  "rewrites": [
    { "source": "/", "destination": "/onboarding-portal.html" },
    { "source": "/pitch", "destination": "/onboarding-portal-pitch.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
